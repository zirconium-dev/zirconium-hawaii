# This is the system preset.

enable rechunker-group-fix.service

# Keeping systemd-networkd enabled with NetworkManger breaks systemd-timesyncd for some reason
enable NetworkManager.service
disable systemd-networkd.service
disable systemd-networkd.socket
disable systemd-networkd-varlink.socket
disable systemd-networkd-resolve-hook.socket
disable systemd-network-generator.service
disable systemd-networkd-wait-online.service
disable systemd-networkd-persistent-storage.service
